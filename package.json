{
  "name": "project-generator",
  "version": "1.0.0",
  "description": "Project for generating any node app having predefined boilerplate",
  "main": "./src/index.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --debounce --clear ./src/index.ts",
    "test": "ts-node ./src/index.ts",
    "build": "tsc",
    "start": "node ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calconcrack/node-project-generator.git"
  },
  "keywords": [
    "node",
    "generator",
    "generate",
    "boilerplate",
    "initiator",
    "initiate",
    "init"
  ],
  "author": "lyadhGod",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/calconcrack/node-project-generator/issues"
  },
  "homepage": "https://github.com/calconcrack/node-project-generator#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-config-google": "^0.14.0",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  }
}
